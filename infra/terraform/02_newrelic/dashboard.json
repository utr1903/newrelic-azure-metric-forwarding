          "title": "Read IOPS",
          "layout": {
            "column": 1,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": YOUR_ACCOUNT_ID,
                "query": "FROM Metric SELECT average(azure.postgresserver.flexible.read_iops) WHERE metricName = 'azure.postgresserver.flexible.read_iops' SINCE 30 minutes ago TIMESERIES "
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "yAxisLeft": {
              "zero": true
            }
